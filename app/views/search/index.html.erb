<h1>Results</h1>

<table class="results_table">
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Fuel Type</th>
    <th>Distance</th>
    <th>Access Times</th>
  </tr>

  <% @presenter.stations.each do |station| %>
    <tr class="station_row">
      <td><%= station.name %></td>
      <td><%= station.address %></td>
      <td><%= station.fuel_type %></td>
      <td><%= station.distance %></td>
      <td><%= station.access_times %></td>
    </tr>
  <% end %>
</table>
